<?xml version="1.0"?>
<block>
  <name>pdu_round_robin</name>
  <key>iridium_pdu_round_robin</key>
  <category>iridium</category>
  <import>import iridium</import>
  <make>iridium.pdu_round_robin($output_count)</make>

  <param>
    <name>Output Count</name>
    <key>output_count</key>
	<value>2</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out_0</name>
    <type>message</type>
  </source>

  <source>
    <name>out_1</name>
    <type>message</type>
  </source>

</block>
